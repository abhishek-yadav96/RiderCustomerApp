<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".DocumentDetailsFragment">
    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <androidx.constraintlayout.widget.ConstraintLayout

            android:layout_width="match_parent"
            android:layout_height="match_parent">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/mainlayout"
        android:paddingVertical="@dimen/_5sdp"
        android:elevation="10dp"
        android:background="@color/white"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="21dp"
            android:layout_marginStart="@dimen/_15sdp"
            android:id="@+id/image"
            app:layout_constraintStart_toStartOf="parent"
            android:background="@drawable/vector__66_"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/documents_details"
            android:textSize="24dp"
            android:id="@+id/profile"
            app:layout_constraintStart_toEndOf="@+id/image"
            app:layout_constraintTop_toTopOf="@id/image"
            app:layout_constraintBottom_toBottomOf="@id/image"
            app:layout_constraintTop_toBottomOf="@id/image"
            android:layout_marginStart="@dimen/_14sdp"
            android:fontFamily="@font/inter_semibold"
            android:textColor="@color/black"/>
    </androidx.constraintlayout.widget.ConstraintLayout>


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:id="@+id/mainlayout1"
        android:padding="20dp"
        android:layout_marginHorizontal="@dimen/_15sdp"
        android:background="@drawable/border"
        app:layout_constraintTop_toBottomOf="@id/mainlayout">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="RC"
            android:textSize="14sp"
            app:layout_constraintStart_toStartOf="parent"
            android:fontFamily="@font/inter_semibold"
            android:textColor="@color/black"
            android:id="@+id/handovercard"
            app:layout_constraintTop_toTopOf="parent"
           />
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@id/handovercard"
            app:layout_constraintBottom_toBottomOf="@id/handovercard"
            android:background="@drawable/group_34080"/>
    </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/mainlayout2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="@dimen/_15sdp"
                android:layout_marginTop="11dp"
                android:background="@drawable/border"
                android:padding="20dp"
                app:layout_constraintEnd_toEndOf="@+id/mainlayout1"
                app:layout_constraintStart_toStartOf="@+id/mainlayout1"
                app:layout_constraintTop_toBottomOf="@id/mainlayout1">

                <TextView
                    android:id="@+id/handovercard1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/inter_semibold"
                    android:text="Driving License"
                    android:textColor="@color/black"
                    android:textSize="14sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/group_34080"
                    app:layout_constraintBottom_toBottomOf="@id/handovercard1"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="@id/handovercard1" />
            </androidx.constraintlayout.widget.ConstraintLayout>


            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/mainlayout3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="@dimen/_15sdp"
                android:layout_marginTop="11dp"
                android:background="@drawable/border"
                android:padding="20dp"
                app:layout_constraintEnd_toEndOf="@+id/mainlayout2"
                app:layout_constraintStart_toStartOf="@+id/mainlayout2"
                app:layout_constraintTop_toBottomOf="@id/mainlayout2">

                <TextView
                    android:id="@+id/handovercard2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/inter_semibold"
                    android:text="Aadhar"
                    android:textColor="@color/black"
                    android:textSize="14sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/group_34080"
                    app:layout_constraintBottom_toBottomOf="@id/handovercard2"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="@id/handovercard2" />


            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/mainlayout4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="@dimen/_15sdp"
                android:layout_marginTop="11dp"
                android:background="@drawable/border"
                android:padding="20dp"
                app:layout_constraintEnd_toEndOf="@+id/mainlayout3"
                app:layout_constraintStart_toStartOf="@+id/mainlayout3"
                app:layout_constraintTop_toBottomOf="@id/mainlayout3">

                <TextView
                    android:id="@+id/handovercard3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/inter_semibold"
                    android:text="Pan"
                    android:textColor="@color/black"
                    android:textSize="14sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/group_34080"
                    app:layout_constraintBottom_toBottomOf="@id/handovercard3"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="@id/handovercard3" />

            </androidx.constraintlayout.widget.ConstraintLayout>
<View
    android:layout_width="match_parent"
    android:layout_height="1dp"
    android:background="#EEEEEE"
    android:id="@+id/view1"
    android:layout_marginHorizontal="18dp"
    android:layout_marginTop="24dp"
    app:layout_constraintTop_toBottomOf="@id/mainlayout4"/>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/mainlayout6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="@dimen/_13sdp"
                android:layout_marginTop="24dp"
                android:background="@drawable/border"
                android:padding="20dp"
                app:layout_constraintEnd_toEndOf="@+id/mainlayout4"
                app:layout_constraintStart_toStartOf="@+id/mainlayout4"
                app:layout_constraintTop_toBottomOf="@id/view1">

                <TextView
                    android:id="@+id/handovercard5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/inter_semibold"
                    android:text="Phone Number"
                    android:textColor="@color/black"
                    android:textSize="14sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/inter_semibold"
                    android:text="+91 954000527"
                    android:textColor="#686868"
                    android:textSize="14sp"
                    app:layout_constraintStart_toStartOf="@id/handovercard5"
                    app:layout_constraintTop_toBottomOf="@id/handovercard5" />
            </androidx.constraintlayout.widget.ConstraintLayout>


            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/mainlayout7"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="@dimen/_13sdp"
                android:layout_marginTop="11dp"
                android:background="@drawable/border"
                android:padding="20dp"
                app:layout_constraintEnd_toEndOf="@+id/mainlayout6"
                app:layout_constraintStart_toStartOf="@+id/mainlayout6"
                app:layout_constraintTop_toBottomOf="@id/mainlayout6">

                <TextView
                    android:id="@+id/handovercard6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/inter_semibold"
                    android:text="Gender"
                    android:textColor="@color/black"
                    android:textSize="14sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/inter_semibold"
                    android:text="Male"
                    android:textColor="#686868"
                    android:textSize="14sp"
                    app:layout_constraintStart_toStartOf="@id/handovercard6"
                    app:layout_constraintTop_toBottomOf="@id/handovercard6" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/mainlayout8"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="@dimen/_13sdp"
                android:layout_marginTop="11dp"
                android:background="@drawable/border"
                android:padding="20dp"
                app:layout_constraintEnd_toEndOf="@+id/mainlayout7"
                app:layout_constraintStart_toStartOf="@+id/mainlayout7"
                app:layout_constraintTop_toBottomOf="@id/mainlayout7">

                <TextView
                    android:id="@+id/handovercard7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/inter_semibold"
                    android:text="Address"
                    android:textColor="@color/black"
                    android:textSize="14sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/inter_semibold"
                    android:text="Delhi, India"
                    android:textColor="#686868"
                    android:textSize="14sp"
                    app:layout_constraintStart_toStartOf="@id/handovercard7"
                    app:layout_constraintTop_toBottomOf="@id/handovercard7" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:background="@drawable/vector__43_"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />
            </androidx.constraintlayout.widget.ConstraintLayout>


            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/mainlayout9"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="@dimen/_13sdp"
                android:layout_marginTop="11dp"
                android:layout_marginBottom="60dp"
                android:background="@drawable/border"
                android:padding="20dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/mainlayout8"
                app:layout_constraintHorizontal_bias="0.666"
                app:layout_constraintStart_toStartOf="@+id/mainlayout8"
                app:layout_constraintTop_toBottomOf="@id/mainlayout8"
                app:layout_constraintVertical_bias="1.0">

                <TextView
                    android:id="@+id/handovercard8"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/inter_semibold"
                    android:text="Language | speak"
                    android:textColor="@color/black"
                    android:textSize="14sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/inter_semibold"
                    android:text="Language details not available"
                    android:textColor="#686868"
                    android:textSize="14sp"
                    app:layout_constraintStart_toStartOf="@id/handovercard8"
                    app:layout_constraintTop_toBottomOf="@id/handovercard8" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:background="@drawable/vector__43_"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />
            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.core.widget.NestedScrollView>


</androidx.constraintlayout.widget.ConstraintLayout>